{
	"action"   : "create",
	"create"   : "database",
	"teams"    : ["oci"],
	"parent"   : "oci.@{model}.data.hk",
	"database" : {
		"name"    : "full",
		"label"   : "Full",
		"desc"    : "OCI @{desc} full housekeeping database.",
		"format"  : "{t} {hk_id} {value}",
		"fields"  : [
			{
				"name" : "t_id",
				"type" : "int(4)"
			},
			{
				"name" : "t",
				"type" : "instant(us)"
			},
			{
				"name" : "hk_id",
				"type" : "int(4)"
			},
			{
				"name" : "samples",
				"type" : "int(2)"
			},
			{
				"name" : "value",
				"type" : "float(8)",
				"nul"  : true
			}
		],
		"indexes" : [
			["t_id"],
			["t"],
			["hk_id", "t"]
