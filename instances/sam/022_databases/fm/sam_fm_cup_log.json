{
  "action": "create",
  "create": "database",
  "parent": "sam.fm.data",
  "database": {
    "name": "cup_log",
    "label": "Cup Log",
    "desc": "SAM flight model cup activity log database.",
    "event": true,
    "link": true,
    "log": true,
    "notify": true,
    "subscribe": true,
    "tag": true,
    "track": true,
    "trash": true,
    "wall": true,
    "format": "{Time} {Event} {Cup No}",
    "fields": [
      {
        "name": "Time",
        "type": "datetime"
      },
      {
        "name": "Event",
        "type": "utf8string(16)"
      },
      {
        "name": "Cup No",
        "type": "int(1)"
      },
      {
        "name": "Cup Type",
        "type": "utf8string(16)",
        "nul": true
      },
      {
        "name": "Reagent",
        "type": "utf8string(16)",
        "nul": true
      },
      {
        "name": "TID",
        "type": "int(4)",
        "nul": true
      },
      {
        "name": "Load",
        "type": "int(4)",
        "nul": true
      },
      {
        "name": "Sample ID",
        "type": "utf8string(16)",
        "nul": true
      },
      {
        "name": "Aliquots",
        "type": "int(4)",
        "nul": true
      },
      {
        "name": "Sample Used",
        "type": "boolean",
        "nul": true
      },
      {
        "name": "Sample Desc",
        "type": "utf8string",
        "nul": true
      }
    ],
    "indexes": [["Time"], ["Event"], ["Cup No"]]
  }
}
