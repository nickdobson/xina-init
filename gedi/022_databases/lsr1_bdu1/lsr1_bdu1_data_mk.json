{
    "action"   : "create",
    "create"   : "database",
    "teams"    : ["gedi"],
    "parent"   : "lsr1_bdu1.data",
    "database" : {
        "name"      : "mk",
        "label"     : "Marker",
        "desc"      : "GEDI Laser 1 - BDU 1 marker database.",
        "format"    : "{t_start} {marker_id} {label}",
        "singular"  : "marker",
        "order" : [
            [ "t_start", "desc" ]
        ],
        "fields" : [
            {
                "name"  : "marker_id",
                "type"  : "int(4)"
            },
            {
                "name"  : "label",
                "type"  : "utf8string(64)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "t_start",
                "type"  : "datetime",
                "index" : true
            },
            {
                "name"  : "t_end",
                "type"  : "datetime",
                "index" : true
            },
            {
                "name"  : "notes",
                "type"  : "utf8text",
                "nul"   : true
            }
        ]
    }
}