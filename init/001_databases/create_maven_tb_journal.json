{
    "action": "create",
    "create": "database",
    "team":   "MAVEN",
    "database": {
        "name":          "maven_tb_journal",
        "label":         "MAVEN TB Journal",
        "desc":          "MAVEN testbed activity journal.",
        "enable_lock":   false,
        "enable_log":    true,
        "enable_notify": false,
        "top_keys":      1,
        "objects": {
            "xo_view_conf_journal": {
                "date":    "Date",
                "title":   "Title",
                "content": "Content",
                "meta": [
                    "Hw Config"
                ]
            },
            "xo_view_default": {
                "*": "journal"
            }
        },
        "fields": [
            {
                "name": "Date",
                "type": "date",
                "key":  true
            },
            {
                "name":  "Title",
                "type":  "string(64)",
                "index": true
            },
            {
                "name":  "Content",
                "type":  "text",
                "index": true,
                "nul":   true
            },
            {
                "name":  "Hw Config",
                "type":  "string(16)",
                "index": true,
                "nul":   true
            }
        ]
    }
}