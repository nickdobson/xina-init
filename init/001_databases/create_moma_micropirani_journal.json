{
    "action": "create",
    "create": "database",
    "team":   "MOMA",
    "database": {
        "name":          "moma_micropirani_journal",
        "label":         "MOMA MicroPirani Journal",
        "desc":          "MOMA MicroPirani activity journal.",
        "enable_lock":   false,
        "enable_log":    true,
        "enable_notify": false,
        "top_keys":      1,
        "objects" : {
            "xo_view_conf_note" : {
                "date"      : "Date",
                "timestamp" : "Timestamp",
                "content"   : "Content",
                "meta"      : [
                    "WOA",
                    "Proc ID",
                    "Hw Config"
                ]
            },
            "xo_view_default": {
                "*" : "note"
            }
        },
        "fields" : [
            {
                "name"  : "Date",
                "type"  : "date",
                "index" : true
            },
            {
                "name" : "Timestamp",
                "type" : "datetime",
                "key"  :  true
            },
            {
                "name"  : "Content",
                "type"  : "text",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "WOA",
                "type"  : "string(64)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "Proc ID",
                "type"  : "string(32)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "Hw Config",
                "type"  : "string(16)",
                "index" : true,
                "nul"   : true
            }
        ]
    }
}