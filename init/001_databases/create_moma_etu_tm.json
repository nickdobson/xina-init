{
    "action"   : "create",
    "create"   : "database",
    "team"     : "MOMA",
    "database" : {
        "name"          : "moma_etu_tm",
        "label"         : "MOMA ETU Telemetry",
        "desc"          : "MOMA ETU model telemetry database.",
        "entry_format"  : "{TID}-{TID Text}",
        "enable_lock"   : false,
        "enable_log"    : true,
        "enable_notify" : true,
        "top_keys"      : 1,
        "objects"       : {
            "repo_entry"       : "{Timestamp}-{TID}-{TID Text}",
            "os_win_repo_path" : "{$doc}{$}MOMA{$}data",
            "os_osx_repo_path" : "{$home}{$}MOMA{$}data",
            "os_nix_repo_path" : "{$home}{$}gse{$}data"
        },
        "fields" : [
            {
                "name"  : "TID",
                "type"  : "int(4)",
                "key"   : true
            },
            {
                "name"  : "TID Text",
                "type"  : "string(64)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "Timestamp",
                "type"  : "string(32)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "Date",
                "type"  : "localdate",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "DOY",
                "type"  : "int(2)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "Model",
                "type"  : "string(16)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"    : "Op Type",
                "type"    : "string(4)",
                "index"   : true,
                "nul"     : true,
                "options" : [
                    {
                        "value" : "stby",
                        "desc"  : "standby"
                    },
                    {
                        "value" : "sci",
                        "desc"  : "science"
                    },
                    {
                        "value" : "eng",
                        "desc"  : "engineering"
                    },
                    {
                        "value" : "cal",
                        "desc"  : "calibration"
                    },
                    {
                        "value" : "bkg",
                        "desc"  : "background"
                    }
                ]
            },
            {
                "name"  : "Script",
                "type"  : "string(64)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "Script Rev",
                "type"  : "string(16)",
                "nul"   : true
            },
            {
                "name"  : "Cfg Script",
                "type"  : "string(64)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "Cfg Script Rev",
                "type"  : "string(16)",
                "nul"   : true
            },
            {
                "name"  : "Lib Script",
                "type"  : "string(64)",
                "index" : true,
                "nul"   : true
            },
            {
                "name"  : "Lib Script Rev",
                "type"  : "string(16)",
                "nul"   : true
            },
            {
                "name"  : "t_utc_start",
                "type"  : "int(8)",
                "nul"   : true
            },
            {
                "name"  : "t_utc_end",
                "type"  : "int(8)",
                "nul"   : true
            },
            {
                "name"  : "t_sclk_start",
                "type"  : "int(8)",
                "nul"   : true
            },
            {
                "name"  : "t_sclk_end",
                "type"  : "int(8)",
                "nul"   : true
            },
            {
                "name"  : "dt",
                "type"  : "int(8)",
                "nul"   : true
            },
            {
                "name"  : "Comments",
                "type"  : "text",
                "nul"   : true
            }
        ]
    }
}