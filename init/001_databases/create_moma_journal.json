{
    "action": "create",
    "create": "database",
    "team": "MOMA",
    "database": {
        "name": "moma_journal",
        "label": "MOMA Journal",
        "desc": "MOMA engineering journal.",
        "enable_lock": false,
        "enable_log": true,
        "enable_notify": false,
        "top_keys": 1,
        "objects": {
            "xo_view_conf_journal": {
                "date": "Date",
                "title": "Title",
                "content": "Content",
                "meta": [
                    "WOA",
                    "Proc ID",
                    "Hw Config"
                ]
            },
            "xo_view_default": {
                "*": "journal"
            }
        },
        "fields": [
            {
                "name": "Date",
                "type": "date",
                "key": true
            },
            {
                "name": "Title",
                "type": "string(64)",
                "index": true
            },
            {
                "name": "Content",
                "type": "text",
                "index": true,
                "nul": true
            },
            {
                "name": "WOA",
                "type": "string(64)",
                "index": true,
                "nul": true
            },
            {
                "name": "Proc ID",
                "type": "string(32)",
                "index": true,
                "nul": true
            },
            {
                "name": "Hw Config",
                "type": "string(16)",
                "index": true,
                "nul": true
            }
        ]
    }
}