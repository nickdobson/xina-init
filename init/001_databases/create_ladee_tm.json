{
    "action": "create",
    "create": "database",
    "team": "LADEE",
    "database": {
        "name": "ladee_tm",
        "label": "LADEE Telemetry",
        "desc": "LADEE telemetry database.",
        "entry_format": "{TID}-{TID Text}",
        "enable_lock": false,
        "enable_log": true,
        "enable_notify": true,
        "top_keys": 1,
        "objects": {
            "repo_entry": "{Timestamp:YYYY-MM-dd-HH.mm.ss}-{TID}-{TID Text}",
            "os_win_repo_path": "{$doc}{$}LADEE{$}data",
            "os_osx_repo_path": "{$home}{$}LADEE{$}data",
            "os_nix_repo_path": "{$home}{$}gse{$}data"
        },
        "fields": [
            {
                "name": "TID",
                "type": "int(4)",
                "key": true
            },
            {
                "name": "TID Text",
                "type": "string(64)",
                "index": true,
                "nul": true
            },
            {
                "name": "Timestamp",
                "type": "localdatetime",
                "index": true,
                "nul": true
            },
            {
                "name": "Date",
                "type": "localdate",
                "index": true,
                "nul": true
            },
            {
                "name": "DOY",
                "type": "int(2)",
                "index": true,
                "nul": true
            },
            {
                "name": "RT TID",
                "type": "int(4)",
                "nul": true
            },
            {
                "name": "Script",
                "type": "string(64)",
                "index": true,
                "nul": true
            },
            {
                "name": "Script Rev",
                "type": "string(16)",
                "nul": true
            },
            {
                "name": "Cfg Script",
                "type": "string(64)",
                "index": true,
                "nul": true
            },
            {
                "name": "Cfg Script Rev",
                "type": "string(16)",
                "nul": true
            },
            {
                "name": "Script Lib",
                "type": "string(64)",
                "index": true,
                "nul": true
            },
            {
                "name": "Script Lib Rev",
                "type": "string(16)",
                "nul": true
            },
            {
                "name": "RTS",
                "type": "int(2)",
                "nul": true
            },
            {
                "name": "Comments",
                "type": "text",
                "nul": true
            }
        ]
    }
}