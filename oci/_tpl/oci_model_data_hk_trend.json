{
	"action"   : "create",
	"create"   : "database",
	"teams"    : ["oci"],
	"parent"   : "oci.@{model}.data.hk",
	"database" : {
		"name"        : "trend",
		"label"       : "Trend",
		"desc"        : "OCI @{desc} housekeeping trend database.",
		"file"        : true,
		"file_format" : "{name}",
		"format"      : "{label}",
		"order"       : [["label", "desc"]],
		"fields"      : [
			{
				"name" : "label",
				"type" : "utf8vstring(64)",
				"key"  : true
			},
			{
				"name" : "trend",
				"type" : "utf8vstring(64)"
			},
			{
				"name" : "name",
				"type" : "utf8vstring(128)"
			},
			{
				"name" : "profiles",
				"type" : "set(asciistring(32))"
			},
			{
				"name" : "t",
				"type" : "jsonarray",
				"nul"  : true
			},
			{
				"name" : "intervals",
				"type" : "jsonarray",
				"nul"  : true
			},
			{
				"name" : "plot",
				"type" : "jsonobject",
				"nul"  : true
			},
			{
				"name" : "disable_filter",
				"type" : "boolean",
				"def"  : "false"
			},
			{
				"name" : "bin_count",
				"type" : "int(4)",
				"nul"  : true
			},
			{
				"name" : "desc",
				"type" : "utf8text",
				"nul"  : true
			}
		],
		"indexes" : [
			["label"],
			["trend"]
		]
	}
}