{
  "action"   : "create",
  "create"   : "database",
  "teams"    : ["oci"],
  "parent"   : "oci.fpa.data.hk",
  "database" : {
    "name"   : "time",
    "label"  : "Bin Time",
    "desc"   : "OCI FPA model time binned housekeeping database.",
    "format" : "{t} {hk_id} {avg} ({min}, {max})",
    "fields" : [
      {
        "name" : "t_start",
        "type" : "int(8)"
      },
      {
        "name" : "t_end",
        "type" : "int(8)"
      },
      {
        "name" : "duration",
        "type" : "int(8)"
      },
      {
        "name" : "hk_id",
        "type" : "int(4)"
      },
      {
        "name" : "samples",
        "type" : "int(4)"
      },
      {
        "name" : "avg",
        "type" : "float(8)"
      },
      {
        "name" : "min",
        "type" : "float(8)"
      },
      {
        "name" : "max",
        "type" : "float(8)"
      }
