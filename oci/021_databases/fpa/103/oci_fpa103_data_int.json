{
  "action"   : "create",
  "create"   : "database",
  "teams"    : ["oci"],
  "parent"   : "oci.fpa103.data",
  "database" : {
    "name"   : "int",
    "label"  : "Interval",
    "desc"   : "OCI FPA SN103 model interval database.",
    "format" : "{t_start%*us} {pri_id},{sec_id} {label}",
    "fields" : [
      {
        "name" : "t_id",
        "type" : "int(4)",
        "nul"  : true
      },
      {
        "name" : "u_int_id",
        "type" : "int(8)"
      },
      {
        "name" : "int_id",
        "type" : "int(4)"
      },
      {
        "name" : "pri_id",
        "type" : "int(4)"
      },
      {
        "name" : "sec_id",
        "type" : "int(2)"
      },
      {
        "name" : "t_start",
        "type" : "int(8)"
      },
      {
        "name" : "t_end",
        "type" : "int(8)"
      },
      {
        "name" : "duration",
        "type" : "int(8)"
      },
      {
        "name" : "type",
        "type" : "int(1)",
        "def"  : "0"
      },
      {
        "name" : "label",
        "type" : "utf8string(64)"
      },
      {
        "name" : "desc",
        "type" : "utf8text",
        "nul"  : true
      },
      {
        "name" : "meta",
        "type" : "json",
        "nul"  : true
      }
    ],
    "indexes" : [
      ["t_id"],
      ["u_int_id"],
      ["int_id"],
      ["pri_id", "sec_id"]
    ]
  }
}